	Keywords = {Computational Biology/*methods Genome/*genetics Salmonella typhi/genetics *Software},
	Number = {1},
	Pages = {119-20},
	Title = {DNAPlotter: circular and linear interactive genome visualization},
	Volume = {25},
	Year = {2009}}

@article{Casey2001,
	Abstract = {Haemodialysis patients are subject to many restrictions and face a
	lifetime attempting to adhere to various regimes. This small study
	was designed to explore the relationship between fluid gain, compliance
	and staff reinforcement. This was achieved by measuring dietetic
	and nursing educational input using interdialytic weight gain as
