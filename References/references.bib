	Journal-Full = {Cell},
	Mesh = {Bacteria; Binding Sites; DNA, Bacterial; Escherichia coli Proteins; Evolution, Molecular; Gene Expression Regulation, Bacterial; Genes, Regulator; Peptide Chain Elongation, Translational; Peptide Elongation Factors; Phylogeny; RNA, Bacterial; Templates, Genetic; Trans-Activators; Transcription, Genetic},
	Month = {Apr},
