@article{Westesson2012,
	Abstract = {We describe handalign, a software package for Bayesian reconstruction
	of phylogenetic history. The underlying model of sequence evolution
	describes indels and substitutions. Alignments, trees and model parameters
	are all treated as jointly dependent random variables and sampled
	via Metropolis-Hastings Markov chain Monte Carlo (MCMC), enabling
	systematic statistical parameter inference and hypothesis testing.
	handalign implements several different MCMC proposal kernels, allows
	sampling from arbitrary target distributions via Hastings ratios,
	and uses standard file formats for trees, alignments and models.
	AVAILABILITY AND IMPLEMENTATION: Installation and usage instructions
	are at http://biowiki.org/HandAlign.},
	Author = {Westesson, O. and Barquist, L. and Holmes, I.},
	Journal = {Bioinformatics},
	Keywords = {*Bayes Theorem HIV/genetics HIV Envelope Protein gp120/genetics INDEL Mutation Markov Chains Membrane Glycoproteins/genetics Monte Carlo Method Phylogeny *Sequence Alignment Simian immunodeficiency virus/genetics *Software Viral Envelope Proteins/genetics},
	Number = {8},
	Pages = {1170-1},
	Title = {HandAlign: Bayesian multiple sequence alignment, phylogeny and ancestral reconstruction},
	Volume = {28},
	Year = {2012}}

