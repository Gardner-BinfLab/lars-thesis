	Keywords = {Animals Bacteriophage P2/classification/genetics/isolation & purification Bacteriophage P22/classification/genetics/isolation & purification Bacteriophage Typing/methods Carbohydrate Sequence Chromosome Mapping Food Microbiology Genome, Viral Genomics/*methods Humans Molecular Sequence Data O Antigens/genetics Phylogeny Prophages/*genetics Salmonella/genetics/immunology/*virology Salmonella Infections/therapy Salmonella Infections, Animal/therapy Salmonella Phages/classification/*genetics/isolation & purification Siphoviridae/classification/genetics/isolation & purification Virology/methods},
	Pages = {133-75},
	Title = {Salmonella phages and prophages--genomics and practical aspects},
	Volume = {394},
